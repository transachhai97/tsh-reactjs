{
  "name": "tsh-reactjs",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "docsify": "docsify serve docs --port 1234",
    "stylelint": "stylelint \"src/**/*.{js,jsx,htm,html,css,sss,less,scss,sass}\" --fix",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --fix",
    "clean": "yarn stylelint && yarn eslint",
    "start": "yarn clean && cross-env BROWSER=none react-app-rewired start",
    "build": "yarn clean && env-cmd -f .env react-app-rewired build",
    "deploy": "yarn build && cp -R docs build && netlify deploy --prod --dir=build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "react-app-rewired": "^2.1.6",
    "redux-logger": "^3.0.6",
    "stylelint": "^13.6.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "tailwindcss-debug-screens": "^1.1.0"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@manaflair/redux-batch": "^1.0.0",
    "@reduxjs/toolkit": "^1.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.8.6",
    "i18next": "^19.7.0",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-toastify": "^6.0.8",
    "tailwindcss": "^1.7.3"
  }
}
